{
  "name": "php-batch-processing",
  "version": "1.0.0",  
  "description": "Executing batch processing",
  "scripts": {
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
        "prepare-commit-msg": "exec < /dev/tty && node_modules/.bin/cz --hook || true"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romarioarruda/php-batch-processing.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/romarioarruda/php-batch-processing/issues"
  },
  "homepage": "https://github.com/romarioarruda/php-batch-processing#readme",
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
